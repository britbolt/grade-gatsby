<header>
    <div id="intro" class="bg-image">
    <h1 class="banner text-center vh-50 pt-2">Your Students</h1>
   <div id="toolbar">
  <button id="remove" class="btn btn-danger" disabled>
    <i class="fa fa-trash"></i> Delete
  </button>
</div>
     <table class="table text-light"
    {{!-- data-url="data/url.json" --}}
    data-id-field="id"
    data-toolbar="#toolbar"
    data-show-columns="true"
    data-show-columns-toggle-all="true"
    data-detail-view="true"
    data-click-to-select="true"
    data-response-handler="responseHandler">
   
    <thead class="align-items-center">
      <tr class="align-items-center">
        <th scope="col" data-field="id" data-sortable="true" data-align="center">Student Names</th>
        <th scope="col" data-field="name" data-editable="true">Math</th>
        <th scope="col" data-field="name" data-editable="true">Science</th>
        <th scope="col" data-field="name" data-editable="true">Spanish</th>
        <th scope="col" data-field="name" data-editable="true">Geography</th>
        <th scope="col" data-field="name" data-editable="true">English</th>

        {{!-- <th class="col-md-7" data-field="description" data-editable="true" data-editable-emptytext="Custom empty text.">Description</th> --}}
      </tr>
      </thead>

      <tbody>
      <tr>
          <th scope="row"  data-field="name" data-sortable="true" data-align="center">Brittany B</th>
          <td data-field="math-grade" data-sortable="true" data-align="center">B+</td>
           <td data-field="science-grade" data-sortable="true" data-align="center">B+</td>
            <td data-field="spanish-grade" data-sortable="true" data-align="center">B+</td>
             <td data-field="geography-grade" data-sortable="true" data-align="center">B+</td>
              <td data-field="english-grade" data-sortable="true" data-align="center">B+</td>
      </tr>
    </tbody>
  </table>

        <h1 class="banner text-center vh-50 pt-2">Your Students</h1>
        <table id="table" class="teacher-table"></table
    </div>
</header>
